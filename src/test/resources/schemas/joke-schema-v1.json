      },
      "punchline": {
        "title": "Punchline",
        "type": "string",
        "default": "",
        "pattern": "^(.+[\\r\\n]*)+$"
      },
      "id": {
        "title": "Id",
        "type": "integer",
        "default": 0
      }
    }
  }

}